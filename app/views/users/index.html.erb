
<div class="container">
	<div class="jumbotron">
		<h2>Users</h2>
	</div>
<table class="table">
	<tr>
		<th>Id</th>
		<th>Name</th>
		<th>Surname</th>
		<th>Nickname</th>
	</tr>
	<% @users.each do |user|%>
		<tr>
			<td>
				<%= user.id%>
			</td>		
			<td>
				<%= user.name%>
			</td>		
			<td>
				<%= user.surname%>
			</td>		
			<td>
				<%= user.nickname%>
			</td>		
		</tr>
	<%end%>
</table>
</div>
