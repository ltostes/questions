
<div class="container">
<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<table class="table">
	<tr>
		<th>Id</th>
		<th>Name</th>
		<th>Surname</th>
		<th>Nickname</th>
	</tr>
	<% @users.each do |user|%>
		<tr>
			<td>
				<%= user.id%>
			</td>		
			<td>
				<%= user.name%>
			</td>		
			<td>
				<%= user.surname%>
			</td>		
			<td>
				<%= user.nickname%>
			</td>		
		</tr>
	<%end%>
</table>
</div>
