<h1>User#index</h1>
<p>Find me in app/views/user/index.html.erb</p>

<table>
  <tr>
    <th>Name</th>
    <th>Surname</th>
    <th>Nickname</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.surname %></td>
      <td><%= user.nickname %></td>
    </tr>
  <% end %>
</table>
